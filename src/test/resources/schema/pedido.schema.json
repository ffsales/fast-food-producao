  "properties": {
    "idObject": {
      "type": "string"
    },
    "id": {
      "type": "object"
    },
    "itens": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "produto": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string"
              },
              "descricao": {
                "type": "string"
              },
              "categoria": {
                "type": "string"
              }
            }
          },
          "quantidade": {
            "type": "integer"
          }
        }
      }
    },
    "dataRecebimento": {
      "type": "object"
    },
    "status": {
      "type": "string"
    }
  }
}